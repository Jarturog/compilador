f vacio inicio() {
	show("Impresion de las primeras 2*3*2=12 letras del abecedario");
	car [2][3][2] matrizCars;
	loop ent i: 0; i < 2; i++ {
		loop ent j: 0; j < 3; j++ {
			loop ent k: 0; k < 2; k++ {
				matrizCars[i][j][k]: 'A'+(car)(((i * 3 * 2) + (j * 2) + k) \ 26);
				show((car[])(matrizCars[i][j][k]));
			}
		}
	}
	# comentario #
}

